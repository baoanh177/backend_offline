<h2>Quản lí user</h2>
<a href="#" class="btn btn-success">Thêm User</a>
<table class="table table-bordered mt-3 w-100">
    <thead class="table-info">
        <tr>
            <th>STT</th>
            <th>Name</th>
            <th>Email</th>
            <th>Status</th>
            <th>Provider</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach((user, index) => { 
            const { name, email, status, Provider } = user.dataValues
        %>
            <tr>
                <td><%=index + 1%></td>
                <td><%=name%></td>
                <td><%=email%></td>
                <td><%=status%></td>
                <td><%=Provider.name%></td>
                <td class="col-2 text-center">
                    <button class="btn btn-warning">Sửa</button>
                    <button class="btn btn-danger">Xóa</button>
                </td>
            </tr>
        <% }) %>
    </tbody>
</table>